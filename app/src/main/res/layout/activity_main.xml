                android:layout_height="0dp"
                android:layout_weight="65"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1163"></LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="65">
                    <ImageView
                        android:id="@+id/page_exit"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/pre_key_back"
                        android:onClick="ImageClicked"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="52"></LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="645"></LinearLayout>
        </LinearLayout>
    </RelativeLayout>

