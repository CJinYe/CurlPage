<?xml version="1.0" encoding="utf-8"?>
<manifest package="icox.cjy.seeobject"
          android:sharedUserId="share.path"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/aap_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.share.resource.EntryActivity"
                  android:configChanges="orientation|screenSize|keyboardHidden"
                  android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name=".SelectorActivityOlde"
                  android:configChanges="orientation|screenSize|keyboardHidden"
                  android:screenOrientation="landscape"/>
        <activity android:name=".ReadActivity"
                  android:configChanges="orientation|screenSize|keyboardHidden"
                  android:screenOrientation="landscape"/>
        <activity android:name=".SelectorActivity"
                  android:configChanges="orientation|screenSize|keyboardHidden"
                  android:screenOrientation="landscape"/>
    </application>

</manifest>